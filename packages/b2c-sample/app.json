{
  "expo": {
    "name": "b2c-sample",
    "slug": "b2c-sample",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "bazaar",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ad.b2c.react.native.sample"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "ad.b2c.react.native.sample"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "authTenant": "<YourTenant>",
      "authAppId": "<YourAppId>"
    },
    "sdkVersion": "45.0.0"
  },
  "name": "b2c-sample"
}